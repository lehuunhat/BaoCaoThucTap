@model IEnumerable<HienTangToc.Models.SalonModel>

<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Salon <span class="caret"></span></a>
    <ul class="dropdown-menu">
        @foreach (var salon in Model)
        {
            <li><a asp-controller="Home" asp-action="Salon" asp-route-id="@salon.Idsl">@salon.Ten</a></li>
        }
    </ul>
</li>
